{
    "name": "demo",
    "version": "1.0.0",
    "author": "zywOo",
    "main": "src/main/main.ts",
    "description": "zywOo's 2022 project,that's it",
    "scripts": {
        "dev": "concurrently vite \"npm run electron:dev\"",
        "build": "npm run render:build && npm run main:build",
