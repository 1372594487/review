        "electron:dev": "electron .",
        "preview": "vite preview"
    },
    "dependencies": {
        "vue": "^3.2.25"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^2.0.0",
        "concurrently": "^7.0.0",
        "electron": "^16.0.7",
        "electron-builder": "^22.14.5",
        "typescript": "^4.4.4",
        "vite": "^2.7.2",
        "vue-tsc": "^0.29.8"
    },
    "build": {
        "appId": "zywOo.2022_vite_electron_demo",
        "directories": {
            "output": "output"
        }
    }
}
